{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "google",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": 0,
        "no-invalid-this": 0,
        "indent": 0,
        "react/react-in-jsx-scope": 1,
        "max-len": 0,
        "react/prop-types": 1,
        "no-multi-spaces": 1,
        "linebreak-style": 0
    }
}